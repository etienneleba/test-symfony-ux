<!DOCTYPE html>
    <html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body data-controller="symfony--ux-swup--swup">
        <div data-controller="scroll-progress" class="scroll-progress"></div>
        <div class="nav">
            <a href="{{ path('index') }}">Index</a>
            <a href="{{ path('chart') }}">Chart</a>
            <a href="{{ path('auto') }}">Stimulus components</a>
            <a href="{{ path('lazyImage') }}">Lazy Image</a>
        </div>
        <main id="swup">
            <div class="container">
                {% block body %}{% endblock %}
            </div>
        </main>


    </body>
</html>
